{
  "name": "sfdx-vue-typescript-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "setup": "npm run sfdx:org:create && npm run deploy",
    "deploy": "npm run build && npm run sfdx:source:push",
    "open": "npm run sfdx:org:open",
    "sfdx:org:create": "sfdx force:org:create -f ./config/project-scratch-def.json -s -a sfdx-vue-typescript-sample-SCRATCH",
    "sfdx:org:open": "sfdx force:org:open --path '/apex/index'",
    "sfdx:source:push": "sfdx force:source:push",
    "sfdx:source:pull": "sfdx force:source:pull"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-typescript": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  }
}
